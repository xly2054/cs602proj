<div>
	<div class="table-responsive">
		<table class="table table-condensed">
			<thead>
				<tr>
					<th>Id</th>
					<th>Customer Name</th>
				</tr>
			</thead>
			<tbody>
				{{#each data}}
					<tr>
						<td>{{this.cid}}</td>
						<td>
							<a href="customers/{{this.id}}/cart">{{this.customerName}}</a>
						</td>
					</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</div>
