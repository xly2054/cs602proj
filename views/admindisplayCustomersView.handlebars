<div>
	<div class="table-responsive">
		<table class="table table-striped table-bordered table-hover">
			<thead class="thead-dark">
				<tr>
					<th>Id</th>
					<th>Customer Name</th>
				</tr>
			</thead>
			<tbody>
				{{#each data}}
					<tr>
						<td>{{this.cid}}</td>
						<td>
							<a href="/admin/customers/{{this.cid}}/orderlist">{{this.customerName}}</a>
						</td>
					</tr>
				{{/each}}
			</tbody>
		</table>
	</div>
</div>
